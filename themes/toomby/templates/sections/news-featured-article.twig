<div class="large-6 medium-12 columns article">
    <img src="{{TimberImage(article.custom.image).src}}" alt="{{TimberImage(article.custom.image).alt}}" />
    <p class="date">
      {{article.post_date}}
    </p>
    <h1>{{article.post_title}}</h1>
    <p>
      {{article.teaser_text}}
    </p>
</div>
